BINDIR=/usr/local/bin
ETCDIR=/usr/local/etc
SHAREDIR=/usr/local/share
MANDIR=/usr/local/share/man
LOCALE=/usr/local/share/locale

OUTFILE=axel
ARCH=Linux

DESTDIR=
LFLAGS=

CFLAGS=-D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64 -Os 
I18N=1
CC=arm-linux-gcc
STRIP=strip
LFLAGS+=-lpthread
